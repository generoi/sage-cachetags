{
  "name": "generoi/sage-cachetags",
  "type": "library",
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/generoi/sage-cachetags",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "autoload": {
    "psr-4": {
      "Genero\\Sage\\CacheTags\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2"
  },
  "suggest": {
    "roots/acorn": "Required for Sage theme integration. Use Bootstrap class for standalone WordPress usage."
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "@composer lint",
    "lint": "vendor/bin/pint --test",
    "lint:fix": "vendor/bin/pint"
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  },
  "extra": {
    "acorn": {
      "providers": [
        "Genero\\Sage\\CacheTags\\CacheTagsServiceProvider"
      ]
    }
  }
}
